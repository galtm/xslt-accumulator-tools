<?acc-name glossentry-position?>
<?acc-parent-uri ../tree-report-example.xsl?>
<?xml-model href="http://docbook.org/xml/5.1/rng/docbook.rng" schematypens="http://relaxng.org/ns/structure/1.0"?>
<?xml-model href="http://docbook.org/xml/5.1/sch/docbook.sch" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<glossary xmlns="http://docbook.org/ns/docbook" version="5.1">
    <title>Glossary</title>
    <para>Here is a list of glossary terms not in alphabetical order. To achieve alphabetical order
        in formatted results, XSLT will sort the entries.</para>
    <glossentry>
        <glossterm>triangle</glossterm>
        <glossdef>
            <para>A polygon having three sides</para>
        </glossdef>
    </glossentry>
    <glossentry>
        <glossterm><emphasis>isosceles</emphasis> triangle</glossterm>
        <glossdef>
            <para>A polygon having three sides, two of which have equal length</para>
        </glossdef>
    </glossentry>
    <glossentry>
        <glossterm>equilateral triangle</glossterm>
        <glossdef>
            <para>A polygon having three sides of equal length</para>
        </glossdef>
    </glossentry>
</glossary>
